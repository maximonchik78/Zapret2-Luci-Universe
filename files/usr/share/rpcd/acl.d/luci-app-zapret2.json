{
    "luci-app-zapret2": {
        "description": "Grant access to Zapret2 LuCI interface",
        "read": {
            "ubus": {
                "luci": ["*"]
            },
            "uci": ["zapret2"]
        },
        "write": {
            "ubus": {
                "luci": ["*"],
                "service": ["*"]
            },
            "uci": ["zapret2"],
            "file": {
                "/etc/config/zapret2": ["write"],
                "/etc/init.d/zapret": ["write"],
                "/usr/bin/zapret": ["exec"]
            }
        }
    }
}
